  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "qantum",
    "qa",
    "testing",
    "automation",
    "ai",
    "machine-learning",
    "bug-prediction",
    "playwright",
    "selenium",
    "cypress",
    "test-automation",
    "quality-assurance",
    "api-testing",
    "e2e-testing",
    "regression-testing",
    "security",
    "encryption",
    "circuit-breaker",
    "dependency-injection",
    "worker-threads",
    "zero-defect",
    "thermal-throttling",
    "docker-orchestration",
    "swarm-execution",
    "real-time-dashboard",
    "hardware-telemetry"
  ],
  "author": "Димитър Продромов (Dimitar Prodromov) <dimitar.prodromov@qantum.dev>",
  "license": "SEE LICENSE IN LICENSE",
  "homepage": "https://qantum.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/papica777-eng/QAntumQATool.git"
  },
  "bugs": {
    "url": "https://github.com/papica777-eng/QAntumQATool/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc",
    "build:prod": "npx ts-node scripts/build-prod.ts",
    "build:enterprise": "npx ts-node scripts/build-enterprise.ts",
    "build:watch": "tsc --watch",
    "dashboard": "npx tsx -e \"import { DashboardServer } from './src/enterprise/dashboard-server.ts'; const d = new DashboardServer(); d.start();\"",
    "license:generate": "npx tsx -e \"import { LicenseManager } from './src/enterprise/license-manager.ts'; const m = new LicenseManager(); const l = m.generateDevLicense('Acronis-Demo', 'acronis@demo.dev'); m.saveLicense(l); console.log('✅ License Generated for Acronis!');\"",
    "license:status": "npx tsx -e \"import { LicenseManager } from './src/enterprise/license-manager.ts'; const m = new LicenseManager(); console.log(m.displayStatus());\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:phase2": "vitest run tests/phase2.test.ts",
    "test:phase3": "vitest run tests/phase3.test.ts",
    "test:legacy": "node test-all.js",
    "test:full": "npm run test && npm run test:legacy",
    "lint": "eslint src/ --ext .ts",
    "lint:fix": "eslint src/ --ext .ts --fix",
    "prepublishOnly": "npm run build && npm run test:full",
    "version": "npm run build"
  },
  "dependencies": {
    "@pinecone-database/pinecone": "^7.0.0",
    "@types/node-schedule": "^2.1.8",
    "@types/nodemailer": "^7.0.9",
    "@types/ws": "^8.18.1",
    "@xenova/transformers": "^2.17.2",
    "axios": "^1.6.0",
    "chokidar": "^5.0.0",
    "dotenv": "^17.2.3",
    "node-schedule": "^2.1.1",
    "nodemailer": "^8.0.0",
    "playwright": "^1.40.0",
    "ws": "^8.18.3",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/node": "^20.19.31",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "esbuild": "^0.24.2",
    "eslint": "^10.0.0",
    "javascript-obfuscator": "^4.1.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ]
